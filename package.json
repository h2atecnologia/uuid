{
  "version": "1.0.1",
  "name": "@lukeed/uuid",
  "repository": "lukeed/uuid",
  "description": "A tiny (230B), fast, and cryptographically secure UUID (v4) generator for Node and the browser",
  "browser": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "uuid.d.ts",
  "umd:name": "uuid",
  "license": "MIT",
  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "https://lukeed.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "node bin",
    "pretest": "npm run build",
    "test:browser": "umu test browser",
    "test:node": "uvu -r esm test node",
    "test": "npm run test:browser && npm run test:node"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "keywords": [
    "guid",
    "browser",
    "isomorphic",
    "rfc4122",
    "random",
    "uuid",
    "uid"
  ],
  "devDependencies": {
    "bundt": "1.0.1",
    "esm": "3.2.25",
    "is-uuid": "1.0.2",
    "premove": "2.0.1",
    "umu": "0.0.1",
    "uvu": "0.3.1"
  }
}
